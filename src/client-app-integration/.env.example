# AI-CORE Client App Integration - Example Environment Configuration
# Copy this file to .env and configure your values

# API Configuration - REQUIRED
VITE_AI_CORE_API_URL=http://localhost:8080/v1
VITE_AI_CORE_WS_URL=ws://localhost:8080/ws
VITE_AI_CORE_API_KEY=your-api-key-here

# Application Configuration
VITE_APP_TITLE=AI-CORE Client Integration Demo
VITE_APP_DESCRIPTION=Production-ready client-app integration sample
VITE_APP_VERSION=1.0.0

# Demo Configuration
# Options: real, mock
VITE_DEMO_MODE=real
VITE_ENABLE_MOCK_DATA=false
VITE_AUTO_CONNECT=true

# Logging Configuration
# Options: debug, info, warn, error
VITE_LOG_LEVEL=info
VITE_ENABLE_CONSOLE_LOGS=true
VITE_MAX_LOG_ENTRIES=100

# WebSocket Configuration
VITE_WS_RECONNECT_ATTEMPTS=3
VITE_WS_RECONNECT_DELAY=2000
VITE_WS_HEARTBEAT_INTERVAL=30000

# Performance Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
VITE_POLL_INTERVAL=1000
VITE_MAX_POLL_ATTEMPTS=120

# UI Configuration
VITE_ANIMATION_ENABLED=true
VITE_SHOW_DEBUG_INFO=false
VITE_ENABLE_KEYBOARD_SHORTCUTS=true

# Security Configuration (Optional - for production)
# VITE_ENABLE_HTTPS=false
# VITE_CSRF_TOKEN=
# VITE_CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Analytics Configuration (Optional)
# VITE_ENABLE_ANALYTICS=false
# VITE_ANALYTICS_API_KEY=
# VITE_ANALYTICS_ENDPOINT=

# Development Configuration
VITE_DEV_TOOLS=true
VITE_HOT_RELOAD=true
VITE_SOURCE_MAPS=true

# Quick Setup Instructions:
# 1. Replace 'your-api-key-here' with your actual AI-CORE API key
# 2. Update API URLs if your AI-CORE instance is running on different ports
# 3. Set VITE_DEMO_MODE=mock for offline demonstrations
# 4. Set VITE_LOG_LEVEL=debug for detailed troubleshooting
