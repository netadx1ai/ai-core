  "clientIntegrationExamples": {
    "javascript_fetch": {
      "description": "Basic JavaScript fetch example",
      "code": "// Create workflow\nconst response = await fetch('http://localhost:8090/v1/workflows', {\n  method: 'POST',\n  headers: {\n    'Content-Type': 'application/json',\n    'X-API-Key': 'mock-api-key'\n  },\n  body: JSON.stringify({\n    title: 'My Blog Post',\n    definition: 'Create a blog post about AI trends',\n    workflow_type: 'blog-post-social'\n  })\n});\n\nconst workflow = await response.json();\nconsole.log('Workflow ID:', workflow.workflow_id);"
    },
    "websocket_connection": {
      "description": "WebSocket connection example",
      "code": "// Connect to WebSocket for real-time updates\nconst ws = new WebSocket('ws://localhost:8091');\n\nws.onmessage = (event) => {\n  const message = JSON.parse(event.data);\n  console.log('WebSocket message:', message);\n  \n  switch (message.type) {\n    case 'workflow_started':\n      console.log('Workflow started:', message.workflow_id);\n      break;\n    case 'step_updated':\n      console.log('Step update:', message.data.step_name);\n      break;\n    case 'workflow_completed':\n      console.log('Workflow completed!');\n      console.log('Results:', message.data.results);\n      break;\n  }\n};"
    },
    "typescript_client": {
      "description": "TypeScript client integration example",
      "code": "interface WorkflowRequest {\n  title: string;\n  definition: string;\n  workflow_type: string;\n  config?: Record<string, unknown>;\n}\n\ninterface WorkflowResponse {\n  workflow_id: string;\n  status: string;\n  message: string;\n  created_at: string;\n}\n\nclass AICoreCient {\n  constructor(private apiKey: string, private baseUrl: string = 'http://localhost:8090/v1') {}\n  \n  async createWorkflow(request: WorkflowRequest): Promise<WorkflowResponse> {\n    const response = await fetch(`${this.baseUrl}/workflows`, {\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/json',\n        'X-API-Key': this.apiKey\n      },\n      body: JSON.stringify(request)\n    });\n    \n    if (!response.ok) {\n      throw new Error(`API Error: ${response.status}`);\n    }\n    \n    return response.json();\n  }\n}"
    }
  },
