# AI-CORE Client App Environment Configuration

# API Configuration
# Base URL for AI-CORE Federation Service (do not include /v1)
VITE_AI_CORE_API_URL=http://localhost:8801

# Service Endpoints (for health checks and direct access)
VITE_FEDERATION_URL=http://localhost:8801
VITE_INTENT_PARSER_URL=http://localhost:8802
VITE_MCP_MANAGER_URL=http://localhost:8804
VITE_MCP_PROXY_URL=http://localhost:8803

# Development Settings
VITE_DEV_MODE=true
VITE_LOG_LEVEL=info

# Authentication (if needed)
# VITE_AUTH_ENABLED=false
# VITE_JWT_SECRET=your-jwt-secret-here

# WebSocket Configuration
VITE_WS_ENABLED=true
VITE_WS_RECONNECT_INTERVAL=5000

# Feature Flags
VITE_ENABLE_MOCK_AUTH=true
VITE_ENABLE_SERVICE_HEALTH_CHECK=true
VITE_ENABLE_REAL_TIME_UPDATES=true

# Branding
VITE_APP_NAME="AI-CORE Dashboard"
VITE_APP_VERSION="1.0.0"

# Monitoring and Analytics
# VITE_ANALYTICS_ENABLED=false
# VITE_SENTRY_DSN=your-sentry-dsn-here

# Performance
VITE_API_TIMEOUT=30000
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_WORKFLOW_POLL_INTERVAL=5000

# Theme
VITE_DEFAULT_THEME=light
