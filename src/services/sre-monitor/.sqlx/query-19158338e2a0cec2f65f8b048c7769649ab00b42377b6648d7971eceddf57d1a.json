{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT id, service_name, alert_type, severity, message, status,\n               created_at, resolved_at, metadata\n        FROM alerts\n        WHERE ($1::text IS NULL OR status = $1)\n        AND ($2::text IS NULL OR service_name = $2)\n        ORDER BY created_at DESC\n        LIMIT 100\n        ",
